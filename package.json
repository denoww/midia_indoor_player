{
  "name": "midia_indoor_player",
  "version": "2.3.0",
  "description": "Player",
  "main": "server.js",
  "private": true,
  "scripts": {
    "start_dev": "npm run compile-coffee & npm run start_node_dev & sleep 3 && npm run start_player_dev",
    "start_prod": "npm run start_node_prod & sleep 3 && npm run start_player_prod",
    "start_node_prod": "nodemon server.js",
    "start_node_dev": "nodemon --inspect server.js",
    "start_player": "npm run start_player_prod",
    "start-electron": "electron --disable-gpu --no-sandbox .",
    "start_player_dev": "chromium-browser http://localhost:4001",
    "start_player_prod": "chromium-browser -kiosk http://localhost:4001",
    "delete_old_images": "coffee ./tasks/delete_old_images.coffee",
    "update_timezone": "coffee ./tasks/update_timezone.coffee",
    "compile-coffee": "coffee -wc app/assets/**"
  },
  "dependencies": {
    "@sentry/browser": "6.2.5",
    "@sentry/electron": "2.4.0",
    "@sentry/integrations": "6.2.5",
    "@sentry/node": "6.2.5",
    "auto-launch": "^5.0.5",
    "coffeescript": "2.5.1",
    "express": "4.17.1",
    "jimp": "0.14.0",
    "md5": "2.3.0",
    "moment": "^2.29.4",
    "nodemon": "^2.0.20",
    "qrcode": "1.4.4",
    "rss-parser": "3.9.0",
    "sc-node-tools": "1.3.8",
    "sharp": "^0.31.1",
    "shelljs": "^0.8.5",
    "url-exists": "1.0.3"
  }
}
